{
  "type": "page",
  "body": [
    {
      "title": "检索条件",
      "type": "form",
      "mode": "inline",
      "target": "my_crud",
      "submitText": "",
      "gap": "lg",
      "body": [
        {
          "type": "input-text",
          "name": "phone",
          "label": "手机号",
          "borderMode": "half",
          "require": true,
          "placeholder": "请输入手机号",
          "size": "lg"
        },
        {
          "type": "input-datetime",
          "label": "日期时间",
          "name": "dateTime",
          "id": "u:19a52e7a9c6c",
          "require": true,
          "size": "lg"
        },
        {
          "label": "",
          "type": "submit",
          "level": "light",
          "icon": "https://suda.cdn.bcebos.com/images%2F2021-01%2Fsearch.svg"
        }
      ]
    },
    {
      "type": "crud",
      "name": "my_crud",
      "api": {
        "url": "/trace/sms",
        "method": "post",
        "data": {
          "receiver": "${phone}",
          "dateTime": "${dateTime}"
        }
      },
      "columns": [
        {
          "name": "businessId",
          "label": "业务ID"
        },
        {
          "name": "content",
          "label": "发送内容"
        },
        {
          "name": "sendTime",
          "label": "发送时间"
        },
        {
          "name": "sendType",
          "label": "发送状态"
        },
        {
          "name": "receiveTime",
          "label": "回执时间"
        },
        {
          "name": "receiveType",
          "label": "回执状态"
        },
        {
          "name": "receiveContent",
          "label": "回执报告"
        }
      ]
    }
  ]
}